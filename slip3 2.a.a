using System;

class Program
{
    static void Main(string[] args)
    {
        // Example usage of the Sum function
        int result = Sum(5, 7);
        Console.WriteLine("The sum of 5 and 7 is: " + result);
    }

    // Function to calculate the sum of two numbers
    static int Sum(int num1, int num2)
    {
        return num1 + num2;
    }
}
